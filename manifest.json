{
	"manifest_version": 2,
	"name": "Single Dog helper",
	"version": "2.5",
	"description": "一个很无聊的插件",
	"icons": {
        "16": "images/dog16.png",
        "48": "images/dog48.png",
        "128": "images/dog128.png"
    },
	"browser_action": {
		"default_icon": {
            "19": "images/dog19.png",
            "38": "images/dog38.png"
        },
		"default_title": "Single Dog helper",
		"default_popup": "popup.html"
	},
	"permissions": [
        "http://aaa.wkquan.com/"
    ],
    "content_scripts": [
    	{
    		"matches":["http://aaa.wkquan.com/Login/*"],
    		"js": ["jquery.min.js","main.js"],
    		"run_at": "document_idle"
    	},
        {
            "matches": ["http://aaa.wkquan.com/*","http://aaa.pk1172.com/*"],
            "js": ["jquery.min.js","nice.js"],
            "run_at": "document_idle"
        },
        {
            "matches": ["http://aaa.wkquan.com/Task/BrushFTask/BrushAcceptManage","http://aaa.pk1172.com/task/taskmanage.html"],
            "js": ["jquery.min.js","han.js"],
            "run_at": "document_idle"

        }
    ]
}
